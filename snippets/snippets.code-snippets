	"table": {
		"scope": "powershell",
		"prefix": "zp table",
		"description": "Create table data source",
		"body": [
			"function Get-ZPTable",
			"{",
			"    param (",
			"        [parameter(Mandatory = \\$false, ParameterSetName = \"Hidden\")]",
			"        [hashtable]\\$config",
			"    )",
			"    $0",
			"    \\$data = Get-Service",
			"    if(\\$data) {",
			"        \\$output = \\$data | Select-Object -Property Name,@{Name=\"Description\";Expression={\\$_.DisplayName}},Status,StartType ",
			"    }",
			"",
			"    return \\$output",
			"",
			"}"
		]
	},
